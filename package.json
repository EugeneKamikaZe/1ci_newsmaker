{
  "name": "reactui",
  "private": true,
  "description": "React UI for my.1ci",
  "main": "index.js",
  "version": "0.0.1",
  "scripts": {
    "start:webpack": "webpack serve --env port=3300",
    "start:vite": "vite",
    "start:dev:server": "node ./config/json-server/index.js",
    "start:dev:server:registration": "json-server-auth -p 8000 ./config/json-server/db.json",
    "start:dev:server:all": "concurrently \"npm run start:dev:server\" \"npm run start:dev:server:registration\"",
    "start:dev:webpack": "concurrently \"npm run start:webpack\" \"npm run start:dev:server:all\"",
    "start:dev:vite": "concurrently \"npm run start:vite\" \"npm run start:dev:server:all\"",
    "build:webpack:dev": "webpack  --env mode=development",
    "build:webpack:prod": "webpack  --env mode=production",
    "build:vite": "vite build",
    "architecture:decruise": "depcruise --progress --include-only \"^src\" --config ./config/depcruise/.dependency-cruiser.js --output-type dot src | dot -T svg > ./build/dependencygraph.svg",
    "architecture:madge": "madge --extensions ts,tsx --image ./build/madge.svg ./src",
    "cypress:open": "npx cypress open --config-file ./config/cypress/cypress.config.ts",
    "cypress:run": "npx cypress run --config-file ./config/cypress/cypress.config.ts",
    "lint:ts": "eslint -c ./config/eslint/.eslintrc.js \"**/*.{ts,tsx}\"",
    "lint:ts:fix": "eslint -c ./config/eslint/.eslintrc.js \"**/*.{ts,tsx}\" --fix --quiet",
    "lint:scss": "npx stylelint --config ./config/stylelint/.stylelintrc.json \"**/*.scss\"",
    "lint:scss:fix": "npx stylelint --config ./config/stylelint/.stylelintrc.json \"**/*.scss\" --fix",
    "lint:fix": "npm run lint:ts:fix && npm run lint:scss:fix",
    "lint:ts:circural": "madge --extensions ts,tsx --circular ./src",
    "lint:ts:circural:image": "madge --extensions ts,tsx --circular --image ./build/curcuralError.svg ./src",
    "test:unit": "jest --config ./config/jest/jest.config.ts",
    "test:ui": "npx loki test",
    "test:ui:ok": "npx loki approve",
    "test:ui:ci": "loki --requireReference --reactUri file:./storybook-static",
    "test:ui:ci:update": "loki update --requireReference --reactUri file:./storybook-static",
    "test:ui:report": "npm run test:ui:json && npm run test:ui:html",
    "test:ui:json": "node scripts/generate-visual-json-report.js",
    "test:ui:html": "reg-cli --from .loki/report.json --report .loki/report.html",
    "prettier": "npx prettier --config ./config/prettier/.prettierrc src --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:ts:fix && npm run lint:scss:fix",
    "storybook": "start-storybook -p 6006 -c ./config/storybook",
    "storybook:build": "build-storybook -c ./config/storybook",
    "prepare": "husky install",
    "generate:slice": "node ./scripts/createSlice/index.js"
  },
  "dependencies": {
    "@react-spring/web": "^9.6.1",
    "@reduxjs/toolkit": "^1.9.5",
    "@use-gesture/react": "^10.2.26",
    "axios": "^0.27.2",
    "bootstrap": "^5.2.3",
    "classnames": "^2.3.2",
    "formik": "^2.2.9",
    "i18next": "^22.4.14",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-http-backend": "^1.4.5",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.4",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.0.0",
    "react-i18next": "^12.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.0",
    "react-virtuoso": "^4.1.1",
    "redux": "^4.2.1",
    "vite": "^4.3.3",
    "vite-plugin-svgr": "^2.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/plugin-transform-typescript": "^7.18.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@cypress/vite-dev-server": "^5.0.2",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.5",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-interactions": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/builder-webpack5": "^6.4.19",
    "@storybook/manager-webpack5": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@storybook/testing-library": "^0.0.13",
    "@svgr/webpack": "^6.2.1",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^13.4.0",
    "@types/circular-dependency-plugin": "^5.0.5",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-redux": "^7.1.25",
    "@types/react-router": "^5.1.18",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-select": "^5.0.1",
    "@types/react-toastify": "^4.1.0",
    "@types/webpack": "^5.28.1",
    "@types/webpack-bundle-analyzer": "^4.6.0",
    "@types/webpack-dev-server": "^4.7.2",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@typescript-eslint/typescript-estree": "^5.51.0",
    "@vitejs/plugin-react": "^4.0.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-i18next-extract": "^0.8.3",
    "circular-dependency-plugin": "^5.2.2",
    "concurrently": "^7.6.0",
    "copy-webpack-plugin": "^11.0.0",
    "cypress": "^12.5.1",
    "dependency-cruiser": "^12.11.0",
    "eslint": "^8.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-boundaries": "^3.1.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-i18next": "^5.1.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-storybook": "^0.6.12",
    "eslint-plugin-ulbi-tv-plugin": "^0.0.10",
    "eslint-plugin-unused-imports": "^2.0.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.0",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "json-server": "^0.17.3",
    "json-server-auth": "^2.1.0",
    "madge": "^6.0.0",
    "mini-css-extract-plugin": "^2.7.5",
    "postcss": "8",
    "prettier": "^2.8.3",
    "prettier-plugin-css-order": "^1.3.0",
    "reg-cli": "^0.17.6",
    "regenerator-runtime": "^0.13.9",
    "resolve-ts-aliases": "^1.0.1",
    "rollup-plugin-visualizer": "^5.7.1",
    "sass": "^1.57.1",
    "sass-loader": "^12.6.0",
    "storybook-addon-sass-postcss": "^0.1.3",
    "storybook-react-i18next": "^1.1.3",
    "style-loader": "^3.3.1",
    "stylelint": "^14.16.1",
    "stylelint-config-standard-scss": "^3.0.0",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.5.0",
    "typescript": "^4.7.4",
    "webpack": "^5.69.1",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "yup": "^0.32.11"
  }
}
