{
  "name": "reactui",
  "private": true,
  "description": "React UI for my.1ci",
  "main": "index.js",
  "version": "0.0.1",
  "scripts": {
    "start:vite": "vite",
    "start:dev:server": "json-server --watch config/json-server/db.json",
    "start:dev:vite": "concurrently \"npm run start:vite\" \"npm run start:dev:server\"",
    "build:vite": "vite build",
    "lint:ts": "eslint -c ./config/eslint/.eslintrc.js \"**/*.{ts,tsx}\"",
    "lint:ts:fix": "eslint -c ./config/eslint/.eslintrc.js \"**/*.{ts,tsx}\" --fix --quiet",
    "lint:scss": "npx stylelint --config ./config/stylelint/.stylelintrc.json \"**/*.scss\"",
    "lint:scss:fix": "npx stylelint --config ./config/stylelint/.stylelintrc.json \"**/*.scss\" --fix",
    "lint:fix": "npm run lint:ts:fix && npm run lint:scss:fix",
    "prettier": "npx prettier --config ./config/prettier/.prettierrc src --check",
    "prettier:fix": "npm run prettier -- --write"
  },
  "dependencies": {
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-toolbar": "^1.0.4",
    "@lexical/list": "^0.12.2",
    "@lexical/react": "^0.12.2",
    "@lexical/selection": "^0.12.2",
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^0.27.2",
    "classnames": "^2.3.2",
    "lexical": "^0.12.2",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.4",
    "react-dom": "^18.0.0",
    "react-redux": "^8.0.5",
    "redux": "^4.2.1",
    "vite": "^4.3.3"
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.5",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-redux": "^7.1.25",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@typescript-eslint/typescript-estree": "^6.4.1",
    "@vitejs/plugin-react": "^4.0.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.48.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-boundaries": "^3.4.0",
    "eslint-plugin-cypress": "^2.14.0",
    "eslint-plugin-i18next": "^6.0.3",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-storybook": "^0.6.13",
    "eslint-plugin-ulbi-tv-plugin": "^0.0.10",
    "eslint-plugin-unused-imports": "^3.0.0",
    "json-server": "^0.17.3",
    "json-server-auth": "^2.1.0",
    "postcss": "8",
    "prettier": "3.0.2",
    "prettier-eslint-cli": "^7.1.0",
    "prettier-plugin-css-order": "^2.0.0",
    "regenerator-runtime": "^0.13.9",
    "resolve-ts-aliases": "^1.0.1",
    "sass": "^1.57.1",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.7.4"
  }
}
